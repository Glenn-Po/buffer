// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Buffer {
  owner       String
  publicOwner String
  type        String
  date        DateTime
  content     String
  expiryDate  DateTime
  isPublic    Boolean
  id          String   @id
}
